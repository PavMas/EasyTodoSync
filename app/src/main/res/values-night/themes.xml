<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Logpass" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#DD8080</item>
        <item name="colorPrimaryVariant">#700000</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" >?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="dividerColor">#FF0000</item>
        <item name="colorButtonNormal">#E8E8E8</item>
    </style>
    <style name="switchTheme" parent="Theme.AppCompat.Light">
        <item name="colorAccent">#BF3939</item>
        <item name="colorSwitchThumbNormal">#E68E8E</item>
    </style>
</resources>